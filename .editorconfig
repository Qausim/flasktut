# top-most editorconfig file
root = true

# Unix-style newlines
[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true


[*.{js,py}]
charset = utf-8
indent_style = space

# python files
[*.py]
indent_size = 4

# json, yaml, yml files
[*.{json,yaml,yml}]
indent_style = space
indent_size = 2